    "gx:policy": "{'@type':'odrl:Policy','odrl:permission':{'odrl:action':'pontus-x:{{ service_type }}','odrl:constraint':{'odrl:leftOperand':'odrl:elapsedTime','odrl:operator':'odrl:eq','odrl:rightOperand':'{{ service_timeout }}'},'odrl:duty':{'odrl:action':'odrl:compensate','odrl:constraint':{'odrl:leftOperand':'odrl:amount','odrl:operator':'odrl:eq','odrl:rightOperand':{{ service_price }},'odrl:unit':'currency:EUR'}},'odrl:target':'{{ did }}'} }",
