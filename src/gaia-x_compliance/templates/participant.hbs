    "gx:headquarterAddress": {
      "gx:streetAddress": "{{ participant_street_address }}",
      "gx:postalCode": "{{ participant_postal_code }}",
      "gx:locality": "{{ participant_locality }}",
      "gx:countrySubdivisionCode": "{{ participant_country_subdivision_code }}"
    },
    "gx:legalAddress": {
      "gx:streetAddress": "{{ participant_street_address }}",
      "gx:postalCode": "{{ participant_postal_code }}",
      "gx:locality": "{{ participant_locality }}",
      "gx:countrySubdivisionCode": "{{ participant_country_subdivision_code }}"
    }
