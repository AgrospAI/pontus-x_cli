  },
  "homepage": "https://github.com/monatheoctocat/my_package",
  "dependencies": {
    "@deltadao/nautilus": "^1.0.2",
    "@gaia-x/did-web-generator": "^1.0.1",
    "@gaia-x/json-web-signature-2020": "^2.1.0",
    "axios": "^1.7.4",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^5.7.2",
